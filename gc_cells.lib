library(gc) {
    cell(NOT) {
        area: 0;
        pin(A) { direction: input; }
        pin(Y) {
            direction: output;
            function: "A'"; }
    }

    cell(XOR) {
        area: 0;
        pin(A) { direction: input; }
        pin(B) { direction: input; }
        pin(Y) {
            direction: output;
            function: "A^B";
        }
    }

    cell(AND) {
        area: 1;
        pin(A) { direction: input; }
        pin(B) { direction: input; }
        pin(Y) {
            direction: output;
            function: "A*B";
        }
    }

    cell(OR) {
        area: 1;
        pin(A) { direction: input; }
        pin(B) { direction: input; }
        pin(Y) {
            direction: output;
            function: "A+B";
        }
    }

}
